{
  "name": "xr/captcha",
  "type": "package",
  "description": "captcha package for laravel10",
  "license": "MIT",
  "authors": [
    {
      "name": "alex",
      "email": "tangxinzhang@126.com"
    }
  ],
  "require": {
    "php": "^7.2|^8.2",
    "ext-gd": "*",
    "illuminate/support": "~5|^6|^7|^8|^9|^10",
    "illuminate/config": "~5|^6|^7|^8|^9|^10",
    "illuminate/container": "^10.0"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Xr\\Captcha\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Xr\\Captcha\\CaptchaServiceProvider"
      ],
      "aliases": {
        "Captcha": "Xr\\Captcha\\Facades\\Captcha"
      }
    }
  }
}